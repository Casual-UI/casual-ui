{
  "name": "casual-ui-svelte",
  "version": "0.0.9",
  "description": "Casual UI for Svelte",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./src/components/*": "./src/components/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "keywords": [
    "svelte",
    "svelte3",
    "ui components",
    "svelte components"
  ],
  "author": "Dongsheng Zhao",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Blackman99/casual-ui.git",
    "directory": "/packages/svelte/ui"
  },
  "publishConfig": {
    "main": "./dist/my-lib.umd.js",
    "module": "./dist/my-lib.es.js",
    "exports": {
      ".*": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.umd.js"
      }
    }
  },
  "devDependencies": {
    "@iconify/json": "^2.1.77",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "casual-i18n": "workspace:*",
    "casual-styles": "workspace:*",
    "casual-types": "workspace:*",
    "casual-ui-svelte": "workspace:*",
    "casual-utils": "workspace:*",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.0",
    "sass": "^1.49.9",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.6",
    "typescript": "^4.7.4",
    "unocss": "^0.43.1",
    "vite": "^3.0.0"
  }
}
